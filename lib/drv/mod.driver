#!/bin/bash
if [[ $(realpath $0) =~ ^(.*)/([^/]+)$ ]]; then
        WORKDIR=${BASH_REMATCH[1]}
fi

driver() {
	case "${1}" in # switch
		params)
			for PARAM in ${INPUTS[@]}; do
				printf "%s\n" "${PARAM}"
			done
		;;
		*) # validate inputs here before run?
			if [[ -n "${NSXHOST}" ]]; then
				run
			fi
		;;
	esac
}
